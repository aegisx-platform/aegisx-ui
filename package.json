{
  "name": "@aegisx/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx run-many --target=serve --projects=api,web --parallel",
    "dev:all": "nx run-many --target=serve --projects=api,web,admin --parallel",
    "dev:api": "nx serve api",
    "dev:web": "nx serve web",
    "dev:admin": "nx serve admin",
    "build": "nx run-many --target=build --all",
    "build:prod": "nx run-many --target=build --all --configuration=production",
    "test": "nx run-many --target=test --all",
    "test:ci": "nx run-many --target=test --all --configuration=ci --coverage --watchAll=false",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "db:migrate": "npx knex migrate:latest",
    "db:rollback": "npx knex migrate:rollback",
    "db:seed": "npx knex seed:run",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed",
    "security:audit": "yarn audit --level moderate",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "prepare": "husky install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "docker:build": "docker-compose -f docker-compose.prod.yml build",
    "docker:build:no-cache": "docker-compose -f docker-compose.prod.yml build --no-cache",
    "setup": "npm run docker:up && sleep 5 && npm run db:migrate && npm run db:seed",
    "version:patch": "npm version patch && git push origin main --tags",
    "version:minor": "npm version minor && git push origin main --tags",
    "version:major": "npm version major && git push origin main --tags",
    "release": "scripts/release.sh",
    "changelog": "scripts/generate-changelog.sh",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~20.1.0",
    "@angular/compiler": "~20.1.0",
    "@angular/core": "~20.1.0",
    "@angular/forms": "~20.1.0",
    "@angular/platform-browser": "~20.1.0",
    "@angular/platform-browser-dynamic": "~20.1.0",
    "@angular/router": "~20.1.0",
    "@fastify/auth": "^5.0.2",
    "@fastify/autoload": "^6.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@types/bcrypt": "^6.0.0",
    "@types/pg": "^8.15.5",
    "bcrypt": "^6.0.0",
    "fastify": "^5.5.0",
    "fastify-plugin": "^5.0.1",
    "fp": "^0.1.0",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "rxjs": "~7.8.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~20.1.0",
    "@angular-devkit/core": "~20.1.0",
    "@angular-devkit/schematics": "~20.1.0",
    "@angular/cli": "~20.1.0",
    "@angular/compiler-cli": "~20.1.0",
    "@angular/language-service": "~20.1.0",
    "@nx/angular": "^21.4.1",
    "@nx/esbuild": "21.4.1",
    "@nx/eslint": "^21.4.1",
    "@nx/jest": "^21.4.1",
    "@nx/js": "^21.4.1",
    "@nx/node": "^21.4.1",
    "@nx/web": "21.4.1",
    "@nx/workspace": "21.4.1",
    "@schematics/angular": "~20.1.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "dotenv": "^17.2.1",
    "esbuild": "^0.19.2",
    "eslint": "^9.34.0",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-preset-angular": "~15.0.0",
    "jest-util": "^30.0.2",
    "nx": "21.4.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.0"
  }
}
