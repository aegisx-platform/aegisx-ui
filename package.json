{
  "name": "@aegisx/source",
  "version": "1.1.1",
  "license": "MIT",
  "packageManager": "pnpm@10.17.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "./scripts/load-env.sh nx run-many --target=serve --projects=api,web --parallel",
    "dev:all": "./scripts/load-env.sh nx run-many --target=serve --projects=api,web,admin --parallel",
    "dev:api": "./scripts/load-env.sh sh -c 'PORT=$API_PORT nx serve api'",
    "dev:web": "./scripts/load-env.sh nx serve web --port='${WEB_PORT:-4200}'",
    "dev:admin": "./scripts/load-env.sh nx serve admin --port='${ADMIN_PORT:-4201}'",
    "build": "nx run-many --target=build --all",
    "build:prod": "nx run-many --target=build --all --configuration=production",
    "test": "nx run-many --target=test --all",
    "test:ci": "nx run-many --target=test --all --configuration=ci --coverage --watchAll=false",
    "test:integration": "jest --config=jest.integration.config.ts --runInBand",
    "test:integration:watch": "jest --config=jest.integration.config.ts --runInBand --watch",
    "test:integration:coverage": "jest --config=jest.integration.config.ts --runInBand --coverage",
    "test:integration:verbose": "JEST_VERBOSE=true jest --config=jest.integration.config.ts --runInBand --verbose",
    "test:unit": "nx run-many --target=test --all --testPathIgnorePatterns='integration'",
    "test:all": "pnpm run test:unit && pnpm run test:integration && pnpm run test:e2e",
    "test:e2e": "nx e2e e2e",
    "test:e2e:ui": "nx e2e-ui e2e",
    "test:e2e:headed": "nx e2e-headed e2e",
    "test:e2e:debug": "nx e2e-debug e2e",
    "test:e2e:update-snapshots": "nx visual-update e2e",
    "test:e2e:auth": "nx e2e e2e --grep=\"Authentication\"",
    "test:e2e:navigation": "nx e2e e2e --grep=\"Navigation\"",
    "test:e2e:visual": "nx e2e e2e --project=\"visual-regression\"",
    "test:e2e:a11y": "nx e2e e2e --project=\"accessibility\"",
    "test:e2e:performance": "nx e2e e2e --project=\"performance\"",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx affected --target=lint --fix",
    "db:migrate": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:latest",
    "db:migrate:prod": "NODE_ENV=production node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:latest",
    "db:migrate:status": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:status",
    "db:rollback": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:rollback",
    "db:rollback:prod": "NODE_ENV=production node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:rollback",
    "db:seed": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js seed:run",
    "db:seed:prod": "NODE_ENV=production node --loader ts-node/esm ./node_modules/knex/bin/cli.js seed:run",
    "db:reset": "pnpm run db:rollback && pnpm run db:migrate && pnpm run db:seed",
    "db:ping": "node -e \"const {Client}=require('pg');const c=new Client({host:process.env.DATABASE_HOST,port:process.env.DATABASE_PORT,user:process.env.DATABASE_USER,password:process.env.DATABASE_PASSWORD,database:process.env.DATABASE_NAME});c.connect().then(()=>{console.log('Database connection OK');c.end();process.exit(0)}).catch((e)=>{console.error('Database connection failed:',e.message);process.exit(1)})\"",
    "db:status": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:status",
    "db:status:prod": "NODE_ENV=production node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:status",
    "db:migrate:container": "docker exec aegisx_api_prod node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:latest",
    "db:seed:container": "docker exec aegisx_api_prod node --loader ts-node/esm ./node_modules/knex/bin/cli.js seed:run",
    "db:status:container": "docker exec aegisx_api_prod node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:status",
    "security:audit": "pnpm audit --audit-level=moderate",
    "release": "scripts/release.sh",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "prepare": "husky",
    "docker:up": "if [ -f docker-compose.instance.yml ]; then docker-compose -f docker-compose.instance.yml up -d; else docker-compose up -d; fi",
    "docker:down": "if [ -f docker-compose.instance.yml ]; then docker-compose -f docker-compose.instance.yml down; else docker-compose down; fi",
    "docker:reset": "if [ -f docker-compose.instance.yml ]; then docker-compose -f docker-compose.instance.yml down -v && docker-compose -f docker-compose.instance.yml up -d; else docker-compose down -v && docker-compose up -d; fi",
    "docker:ps": "if [ -f docker-compose.instance.yml ]; then docker-compose -f docker-compose.instance.yml ps; else docker-compose ps; fi",
    "docker:build": "docker-compose -f docker-compose.prod.yml build",
    "docker:build:no-cache": "docker-compose -f docker-compose.prod.yml build --no-cache",
    "setup": "./scripts/setup-env.sh && pnpm run docker:up && sleep 5 && pnpm run db:migrate && pnpm run db:seed",
    "setup:env": "./scripts/setup-env.sh",
    "monitoring:start": "docker-compose -f docker-compose.monitoring.yml up -d",
    "monitoring:stop": "docker-compose -f docker-compose.monitoring.yml down",
    "monitoring:restart": "pnpm run monitoring:stop && pnpm run monitoring:start",
    "monitoring:logs": "docker-compose -f docker-compose.monitoring.yml logs -f",
    "prod:setup": "sudo ./scripts/production-setup.sh",
    "prod:secrets": "./scripts/secrets-manager.sh generate",
    "prod:deploy": "./scripts/deploy-production.sh",
    "prod:backup": "./scripts/backup-restore.sh backup",
    "prod:health": "./scripts/health-monitor.sh status",
    "prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "prod:status": "docker-compose -f docker-compose.prod.yml ps",
    "version:patch": "pnpm version patch && git push origin main --tags",
    "version:minor": "pnpm version minor && git push origin main --tags",
    "version:major": "pnpm version major && git push origin main --tags",
    "changelog": "scripts/generate-changelog.sh",
    "security:fix": "pnpm audit fix",
    "hooks:skip": "echo \"Use SKIP_HOOKS=1 git push to skip pre-push hooks\"",
    "hooks:test": "pnpm run lint-staged && .husky/pre-push",
    "push:quick": "SKIP_HOOKS=1 git push",
    "push:force": "SKIP_HOOKS=1 git push --force-with-lease",
    "commit:fix": "git commit -m \"fix: \"",
    "commit:feat": "git commit -m \"feat: \"",
    "test:affected": "nx affected --target=test",
    "lint:affected": "nx affected --target=lint",
    "crud": "./bin/cli.js generate",
    "crud:full": "./bin/cli.js generate --package full",
    "crud:events": "./bin/cli.js generate --with-events",
    "crud:import": "./bin/cli.js generate --with-import",
    "crud:list": "./bin/cli.js list-tables",
    "crud:validate": "./bin/cli.js validate",
    "domain:init": "./bin/cli.js domain:init",
    "domain:list": "./bin/cli.js domain:list",
    "db:migrate:inventory": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile-inventory.ts",
    "db:migrate:inventory:rollback": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:rollback --knexfile knexfile-inventory.ts",
    "db:migrate:inventory:status": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:status --knexfile knexfile-inventory.ts",
    "db:seed:inventory": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js seed:run --knexfile knexfile-inventory.ts",
    "inventory:setup": "pnpm run db:migrate:inventory && pnpm run db:seed:inventory",
    "inventory:import-tmt": "node --loader ts-node/esm apps/api/src/database/scripts/inventory-import-tmt.ts",
    "docs:dev": "vitepress dev docs-site",
    "docs:build": "vitepress build docs-site",
    "docs:preview": "vitepress preview docs-site"
  },
